<p id="notice"><%= notice %></p>

<h1>Detalleordens</h1>

<table>
  <thead>
    <tr>
      <th>Id orden</th>
      <th>Id producto</th>
      <th>Producto</th>
      <th>Cant</th>
      <th>Precio</th>
      <th>Id tipo</th>
      <th>Chequeado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @detalleordens.each do |detalleorden| %>
      <tr>
        <td><%= detalleorden.id_orden %></td>
        <td><%= detalleorden.id_producto %></td>
        <td><%= detalleorden.producto %></td>
        <td><%= detalleorden.cant %></td>
        <td><%= detalleorden.precio %></td>
        <td><%= detalleorden.id_tipo %></td>
        <td><%= detalleorden.chequeado %></td>
        <td><%= link_to 'Show', detalleorden %></td>
        <td><%= link_to 'Edit', edit_detalleorden_path(detalleorden) %></td>
        <td><%= link_to 'Destroy', detalleorden, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Detalleorden', new_detalleorden_path %>
<%= link_to 'comparar', comparar_producto_path %>
